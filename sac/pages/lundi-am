<script type="text/javascript">

      $( document ).ready(function() {

        $('.toggle-hidden-article').click(function(){
            $('.hidden-article').slideToggle('slow');
            $('.toggle-hidden-article').fadeToggle()
        });

        $('.toggle-hidden-article-close').click(function(){
            $('.hidden-article').slideToggle('slow');
            $('.toggle-hidden-article').fadeToggle()
        });


        $('a').click(function(){
            $('html, body').animate({
                scrollTop: $( $.attr(this, 'href') ).offset().top
            }, 500);
            return false;
        });

        $('.search-btn').click(function(){
        	console.log("click search");
            $('#search').focus();
        });

        if( $('#quatre-cent-quatre').length )
        {
           console.log("404");
           $('#search').focus();
        }



    });
</script>


 <script>


$(document).bind('ready ajaxComplete', function(){

  $('.Sommaire-image-fond').on({
      mouseenter: function() {
        var color = $(this).data("color")
        background = $(this).data("background")


          $(this).css("background",color)

           },
           mouseleave: function() {

            $(this).css("background",background)

           }
  });


  $( document ).ajaxComplete(function() {
   var $grid = $('.grid').isotope({
      // options
      itemSelector: '.grid-item',
      layoutMode: 'masonry',
    });
    // layout Isotope after each image loads
    $grid.imagesLoaded().progress( function() {
      $grid.isotope('layout');
    });


  });



  $( ".plus_de_contenu" ).last().show();

  $( ".plus_de_contenu" ).on( "click", function() {
    that = $(this);
    var rubrique = $(this).data( "rubrique" );
    console.log("rubrique:"+rubrique);

    $.ajax({
      method: "GET",

      url: "spip.php?page=inc/inc-sommaire3&id_rubrique="+rubrique,

      beforeSend: function( xhr ) {
        that.addClass("loading");
        that.find("i").addClass("fa-spin");
        console.log("en cours ")
      }
    })
    .done(function( data ) {
      $(".plus_de_contenu").hide();
      that.removeClass("loading");
      that.slideUp('slow');
      $(".big-sommaire").last().after(data)
    });

  });
});




</script>
<script>
  /*var vignettes=document.getElementsByClassName("Sommaire-image");
  for(var i=0 ; i<3 ; i++) {
    var vigne="vign"+i.toString();
    console.log(vigne.toString());
    vignettes[i].setAttribute("id",""+vigne.toString());
    console.log(vignettes[i].getAttribute("id"));
    vigne="#"+vigne;
    console.log(vigne);
    $("#vign0").responsiveImg({srcAttribute:"data-src"});

  }*/

  class ResponsiveBackgroundImage {

    constructor(element) {
        this.element = element;
        this.img = element.querySelector('img');
        this.src = '';

        this.img.addEventListener('load', () => {
            this.update();
        });

        if (this.img.complete) {
            this.update();
        }
    }

    update() {
        let src = typeof this.img.currentSrc !== 'undefined' ? this.img.currentSrc : this.img.src;
        if (this.src !== src) {
            this.src = src;
            this.element.style.background = 'url("' + this.src + '")' + "no-repeat center";
            this.element.style.backgroundSize = 'cover';

        }
    }
  }

  let elements = document.querySelectorAll('[data-responsive-background-image]');
  for (let i=0; i<elements.length; i++) {
    new ResponsiveBackgroundImage(elements[i]);
  }

</script>


